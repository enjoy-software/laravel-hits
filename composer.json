{
    "name": "enjoy-software/laravel-hits",
    "description": "Hit model for recording visitor information and tracking page views on various models",
    "keywords": [
        "analytics",
        "hits",
        "laravel",
        "page-views",
        "statistics",
        "visitor-tracking"
    ],
    "homepage": "https://github.com/enjoy-software/laravel-hits",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Enjoy Software",
            "email": "veryenjoysoftware@gmail.com",
            "homepage": "https://github.com/enjoy-software"
        }
    ],
    "support": {
        "issues": "https://github.com/enjoy-software/laravel-hits/issues",
        "source": "https://github.com/enjoy-software/laravel-hits"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/http": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "orchestra/testbench": "^10.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "EnjoySoftware\\LaravelHits\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EnjoySoftware\\LaravelHits\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EnjoySoftware\\LaravelHits\\LaravelHitsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "lint": [
            "@php vendor/bin/pint --ansi"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ]
    }
}